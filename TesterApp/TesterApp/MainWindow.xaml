<Window x:Class="TesterApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TesterApp"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800"
        >
    <Grid>
        
        <WrapPanel Orientation="Vertical" HorizontalAlignment="Center">
            <Label x:Name="label1" Content="Email:" Margin="5" 
                   FontFamily="Roboto Medium"></Label>
            <TextBox x:Name="textbox1" Width="200" Margin="5"  FontFamily="Roboto Light"></TextBox>
            <Label x:Name="label2" Content="Password:" Margin="5"  FontFamily="Roboto Medium"></Label>
            <TextBox x:Name="textbox2"  Width="200" Margin="5"  FontFamily="Roboto Light"></TextBox>
            <Button x:Name="button1" Content="  Begin  " 
                 Margin="20" FontFamily="Roboto Bold"/>
        </WrapPanel>

        

    </Grid>

</Window>
